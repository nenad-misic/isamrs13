  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "string",
      "required": true
    },
    "telephone": {
      "type": "string",
      "required": true
    },
    "city": {
      "type": "string",
      "required": true
    },
    "points": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "type": {
      "type": "string",
      "required": true,
      "default": "regUser"
    }
  },
  "validations": [],
  "relations": {
