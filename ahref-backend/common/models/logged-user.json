      "type": "hasMany",
      "model": "mRoomReservations",
      "foreignKey": ""
    },
    "mFlightReservations": {
      "type": "hasMany",
      "model": "mFlightReservations",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "login"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
