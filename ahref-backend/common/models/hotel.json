{
  "name": "Hotel",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "latitude": {
      "type": "number",
      "required": true
    },
    "longitude": {
      "type": "number",
      "required": true
    },
    "description": {
      "type": "number",
      "required": true
    },
    "rating": {
      "type": "number",
      "required": true,
      "default": 0
    },
    "numOfRates": {
      "type": "number",
      "required": true,
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "rooms": {
      "type": "hasMany",
      "model": "Room",
      "foreignKey": ""
    },
    "priceList": {
      "type": "hasOne",
      "model": "HPriceList",
      "foreignKey": ""
    },
    "destination": {
      "type": "belongsTo",
      "model": "Destination",
      "foreignKey": ""
    },
    "loggedUser": {
      "type": "belongsTo",
      "model": "LoggedUser",
      "foreignKey": ""
    }
  },
  "acls": [],
  "methods": {}
}
